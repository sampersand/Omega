func:guess:(max):{
    (max * om:rand):round + 1 -> rn
    trials <- 0
    whilst:True:{
        input:('Pick a number from 1-' + max) -> gues
        gues:type <- 1:type
        disp:('#',\
              (trials <- trials + 1),\
              x,\
              (if:(x < rn):\
                '<':\
                (if:(x > rn):\
                    '>':\
                    (escape:;'=')\
                    )\
                )
              )
    }
    return:x
}
